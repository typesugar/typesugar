{
  "name": "@typesugar/transformer",
  "version": "0.1.0",
  "description": "ðŸ§Š TypeScript transformer for typesugar macro expansion",
  "author": "Dean Povey <dean.povey@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/typesugar/typesugar.git",
    "directory": "packages/transformer"
  },
  "homepage": "https://github.com/typesugar/typesugar/tree/main/packages/transformer#readme",
  "bugs": {
    "url": "https://github.com/typesugar/typesugar/issues"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "typesugar": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js",
      "require": "./dist/cli.cjs"
    },
    "./language-service": {
      "types": "./dist/language-service.d.ts",
      "import": "./dist/language-service.js",
      "require": "./dist/language-service.cjs"
    },
    "./pipeline": {
      "types": "./dist/pipeline.d.ts",
      "import": "./dist/pipeline.js",
      "require": "./dist/pipeline.cjs"
    },
    "./virtual-host": {
      "types": "./dist/virtual-host.d.ts",
      "import": "./dist/virtual-host.js",
      "require": "./dist/virtual-host.cjs"
    },
    "./position-mapper": {
      "types": "./dist/position-mapper.d.ts",
      "import": "./dist/position-mapper.js",
      "require": "./dist/position-mapper.cjs"
    },
    "./source-map-utils": {
      "types": "./dist/source-map-utils.d.ts",
      "import": "./dist/source-map-utils.js",
      "require": "./dist/source-map-utils.cjs"
    },
    "./cache": {
      "types": "./dist/cache.d.ts",
      "import": "./dist/cache.js",
      "require": "./dist/cache.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "typescript",
    "macros",
    "transformer",
    "ts-patch"
  ],
  "dependencies": {
    "@typesugar/core": "workspace:*",
    "@typesugar/derive": "workspace:*",
    "@typesugar/comptime": "workspace:*",
    "@typesugar/reflect": "workspace:*",
    "@typesugar/operators": "workspace:*",
    "@typesugar/typeclass": "workspace:*",
    "@typesugar/specialize": "workspace:*",
    "@typesugar/mapper": "workspace:*",
    "@typesugar/preprocessor": "workspace:*",
    "@ampproject/remapping": "^2.3.0",
    "esbuild": "^0.20.0"
  },
  "devDependencies": {
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "files": [
    "dist",
    "src",
    "language-service"
  ],
  "sideEffects": false
}
