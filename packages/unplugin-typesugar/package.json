{
  "name": "unplugin-typesugar",
  "version": "0.1.0",
  "description": "ðŸ§Š Bundler integrations for typesugar (Vite, Webpack, esbuild, Rollup)",
  "author": "Dean Povey <dean.povey@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/typesugar/typesugar.git",
    "directory": "packages/unplugin-typesugar"
  },
  "homepage": "https://github.com/typesugar/typesugar/tree/main/packages/unplugin-typesugar#readme",
  "bugs": {
    "url": "https://github.com/typesugar/typesugar/issues"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "macros",
    "vite",
    "webpack",
    "esbuild",
    "rollup",
    "bundler",
    "plugin"
  ],
  "dependencies": {
    "@ampproject/remapping": "^2.3.0",
    "@typesugar/core": "workspace:*",
    "@typesugar/preprocessor": "workspace:*",
    "@typesugar/transformer": "workspace:*",
    "unplugin": "^2.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false
}
