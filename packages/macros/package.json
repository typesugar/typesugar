  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "typescript",
    "macros",
    "metaprogramming",
    "typeclass",
    "derive"
  ],
  "dependencies": {
    "@typesugar/core": "workspace:*",
    "@typesugar/contracts": "workspace:*",
    "@typesugar/mapper": "workspace:*",
    "@typesugar/testing": "workspace:*"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": true
