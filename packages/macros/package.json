{
  "name": "@typesugar/macros",
  "version": "0.1.0",
  "description": "ðŸ§Š Built-in macro implementations for typesugar",
  "author": "Dean Povey <dean.povey@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/dpovey/typesugar.git",
    "directory": "packages/macros"
  },
  "homepage": "https://github.com/dpovey/typesugar/tree/main/packages/macros#readme",
  "bugs": {
    "url": "https://github.com/dpovey/typesugar/issues"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
