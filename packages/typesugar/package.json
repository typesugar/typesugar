{
  "name": "typesugar",
  "version": "0.1.0",
  "description": "ðŸ§Š TypeScript compile-time macros - Scala 3-style metaprogramming for TypeScript",
  "author": "Dean Povey <dean.povey@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/typesugar/typesugar.git",
    "directory": "packages/typesugar"
  },
  "homepage": "https://github.com/typesugar/typesugar/tree/main/packages/typesugar#readme",
  "bugs": {
    "url": "https://github.com/typesugar/typesugar/issues"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "typesugar": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./transformer": {
      "types": "./dist/transformer.d.ts",
      "import": "./dist/transformer.js",
      "require": "./dist/transformer.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "typescript",
    "macros",
    "metaprogramming",
    "compile-time",
    "transformer",
    "scala3"
  ],
  "dependencies": {
    "@typesugar/core": "workspace:*",
    "@typesugar/macros": "workspace:*",
    "@typesugar/transformer": "workspace:*",
    "@typesugar/comptime": "workspace:*",
    "@typesugar/reflect": "workspace:*",
    "@typesugar/derive": "workspace:*",
    "@typesugar/typeclass": "workspace:*",
    "@typesugar/specialize": "workspace:*",
    "unplugin-typesugar": "workspace:*"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false
}
