{
  "name": "@typesugar/kysely-adapter",
  "version": "0.1.0",
  "private": true,
  "description": "ðŸ§Š Kysely adapter for typesugar - type-safe SQL tagged templates that compile to Kysely's RawBuilder",
  "author": "Dean Povey <dean.povey@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/typesugar/typesugar.git",
    "directory": "packages/kysely"
  },
  "homepage": "https://github.com/typesugar/typesugar/tree/main/packages/kysely#readme",
  "bugs": {
    "url": "https://github.com/typesugar/typesugar/issues"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "typescript",
    "macros",
    "kysely",
    "sql",
    "database",
    "type-safe",
    "query-builder"
  ],
  "dependencies": {
    "@typesugar/core": "workspace:*",
    "@typesugar/sql": "workspace:*"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0",
    "kysely": ">=0.27.0"
  },
  "peerDependenciesMeta": {
    "kysely": {
      "optional": true
    }
  },
  "devDependencies": {
    "kysely": "^0.27.0",
    "vitest": "^2.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false
}
