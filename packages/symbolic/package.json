{
  "name": "@typesugar/symbolic",
  "version": "0.1.0",
  "description": "ðŸ§Š Type-safe symbolic mathematics with AST, rendering, evaluation, calculus, and simplification",
  "author": "Dean Povey <dean.povey@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "prebuild": "typesugar preprocess src --outDir .typesugar",
    "build": "tsup && rm -rf .typesugar",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist .typesugar",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@typesugar/core": "workspace:*",
    "@typesugar/macros": "workspace:*",
    "@typesugar/std": "workspace:*",
    "@typesugar/type-system": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "typesugar",
    "symbolic",
    "math",
    "calculus",
    "differentiation",
    "integration",
    "simplification",
    "latex",
    "cas"
  ],
  "sideEffects": false
}
