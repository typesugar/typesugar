{
  "name": "@typesugar/math",
  "version": "0.1.0",
  "description": "ðŸ§Š Comprehensive math types and typeclasses for TypeScript",
  "author": "Dean Povey <dean.povey@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@typesugar/core": "workspace:*",
    "@typesugar/macros": "workspace:*",
    "@typesugar/std": "workspace:*",
    "@typesugar/units": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "typesugar",
    "math",
    "rational",
    "complex",
    "matrix",
    "interval",
    "polynomial",
    "typeclasses"
  ],
  "sideEffects": false
}
