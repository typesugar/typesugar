{
  "name": "@typesugar/sql",
  "version": "0.1.0",
  "description": "ðŸ§Š Doobie-like type-safe SQL DSL with compile-time query building for typesugar",
  "author": "Dean Povey <dean.povey@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/typesugar/typesugar.git",
    "directory": "packages/sql"
  },
  "homepage": "https://github.com/typesugar/typesugar/tree/main/packages/sql#readme",
  "bugs": {
    "url": "https://github.com/typesugar/typesugar/issues"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "macros",
    "sql",
    "database",
    "doobie",
    "query-builder"
  ],
  "dependencies": {
    "@typesugar/core": "workspace:*",
    "@typesugar/macros": "workspace:*"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false
