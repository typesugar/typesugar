{
  "name": "@typesugar/effect",
  "version": "0.1.0",
  "description": "ðŸ§Š Effect-TS adapter for typesugar - do-notation and comprehension syntax",
  "author": "Dean Povey <dean.povey@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/typesugar/typesugar.git",
    "directory": "packages/effect"
  },
  "homepage": "https://github.com/typesugar/typesugar/tree/main/packages/effect#readme",
  "bugs": {
    "url": "https://github.com/typesugar/typesugar/issues"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "typescript",
    "macros",
    "effect",
    "effect-ts",
    "do-notation",
    "comprehension",
    "monads"
  ],
  "dependencies": {
    "@typesugar/core": "workspace:*",
    "@typesugar/graph": "workspace:*",
    "@typesugar/std": "workspace:*",
    "@typesugar/testing": "workspace:*",
    "@typesugar/type-system": "workspace:*"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0",
    "effect": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "effect": {
      "optional": true
    }
  },
  "devDependencies": {
    "effect": "^3.14.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false
}
