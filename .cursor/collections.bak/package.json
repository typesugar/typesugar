{
  "name": "@typesugar/collections",
  "version": "0.1.0",
  "description": "Scala-style collections typeclass hierarchy with zero-cost abstractions",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./typeclasses": {
      "types": "./dist/typeclasses/index.d.ts",
      "import": "./dist/typeclasses/index.js",
      "require": "./dist/typeclasses/index.cjs"
    },
    "./instances": {
      "types": "./dist/instances/index.d.ts",
      "import": "./dist/instances/index.js",
      "require": "./dist/instances/index.cjs"
    },
    "./views": {
      "types": "./dist/views/index.d.ts",
      "import": "./dist/views/index.js",
      "require": "./dist/views/index.cjs"
    },
    "./ops": {
      "types": "./dist/ops/index.d.ts",
      "import": "./dist/ops/index.js",
      "require": "./dist/ops/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@typesugar/type-system": "workspace:*",
    "@typesugar/fp": "workspace:*",
    "@typesugar/std": "workspace:*"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "files": [
    "dist",
    "src"
  ]
}
